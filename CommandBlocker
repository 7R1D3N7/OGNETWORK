on command "/op":
	if player has permission "allow.op":
		stop
	else:
		cancel the event
		message "&9&lPermissions &8&l> &cYou do not have permission for this command."
		
on command "/minecraft:op":
	if player has permission "allow.m.op":
		stop
	else:
		cancel the event
		message "&9&lPermissions &8&l> &cYou do not have permission for this command."		
		
on command "/bukkit:op":
	if player has permission "allow.b.op":
		stop
	else:
		cancel the event
		message "&9&lPermissions &8&l> &cYou do not have permission for this command."
		
